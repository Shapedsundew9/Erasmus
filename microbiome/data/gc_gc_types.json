    "gc_type": {
        "type": ["gc_type_int", "gc_type_str"]
    },
    "gc_type_int": {
        "type": "int32",
        "validate": "x in gc_type_lookup['value']",
        "cast_to": ["gc_type_str"]
    },
    "gc_type_str": {
        "type": "str",
        "validate": "x in gc_type_lookup['name']",
        "cast_to": ["gc_type_int"]
    },
    "gc_value_str": {
        "type": "str",
        "maxlength": 1024,
        "minlength": 1,
        "validate": "exec(x)"
    },
    "gc_signature": {
        "type": "sha256_lowercase",
        "validate": "x in genomic_library"
    },
    "gc_creator": {
        "type": "sha256_lowercase",
        "validate": "x in creators"
    },
    "gc_properties": {
        "type": "dict",
        "schema": {
            "extended": {
                "type": "boolean",
                "default": false
            },
            "constant": {
                "type": "boolean",
                "default": false
            },
            "conditional": {
                "type": "boolean",
                "default": false
            },
            "deterministic": {
                "type": "boolean",
                "default": false
            },
            "memory_modify": {
                "type": "boolean",
                "default": false
            },
            "object_modify": {
                "type": "boolean",
                "default": false
            },
            "physical": {
                "type": "boolean",
                "default": false
            },
            "arithmetic": {
                "type": "boolean",
                "default": false                
            },
            "logical": {
                "type": "boolean",
                "default": false                
            },
            "bitwise": {
                "type": "boolean",
                "default": false                
            }
        }
    },
    "gc_meta_data": {
        "type": "dict",
        "schema": {
            "extended_schema": {"type": "gc_extended_schema"},
            "extended_class": {"type": "gc_extended_class"},
            "parents": {"type": "gc_parents"},
            "name": {"type": "gc_name"}
        }
    },
    "gc_class": {
        "type": "int64"
    },
    "gc_extended_schema": {
        "type": "dict",
        "schema": {
        }
    },
    "gc_extended_class": {
        "type": "dict",
        "schema": {
        }
    },
    "gc_parents_list": {
        "type": "list",
        "schema": {
            "type": "gc_parents"
        }
    },
    "gc_parents": {
        "type": "list",
        "minlength": 1,
        "schema": {
            "type": "sha256_lowercase"
        }
    },
    "letter_uppercase": {
        "type": "str",
        "regex": "[A-Z]",
        "minlength": 1,
        "maxlength": 1
    },
    "timestamp_str":  {
        "type": "str",
        "regex": "20[2-9][0-9]-(1[0-2]|0[1-9])-([0-2][0-9]|3[01])T[0-2][0-9](:[0-5][0-9]){2}\\.[0-9]{6}Z",
        "validate": "datetime.strptime(x, '%Y-%m-%dT%H:%M:%S.%fZ')"
    },
    "gc_evolvability": {
        "type": "float"
    },
    "gc_fitness": {
        "type": "float"
    },
    "gc_updated": {
        "type": "timestamp_str"
    },
    "gc_created": {
        "type": "timestamp_str"
    },
    "sha256_lowercase": {
        "type": "str",
        "regex": "[0-9a-f]{64}"
    },
    "gc_num_inputs": {
        "type": "int16",
        "min": 0,
        "max": 256
    },
    "gc_num_outputs": {
        "type": "int16",
        "min": 0,
        "max": 256
    },
    "gc_reference_count": {
        "type": "int64",
        "min": 0
    },
    "gc_generation": {
        "type": "int64",
        "min": 0
    },
    "gc_code_depth": {
        "type": "int32",
        "min": 0
    },
    "gc_codon_depth": {
        "type": "int32",
        "min": 0
    },
    "gc_num_codes": {
        "type": "int32",
        "min": 0
    },
    "gc_num_unique_codes": {
        "type": "int32",
        "min": 0
    },
    "gc_opt_num_codons": {
        "type": "int32",
        "min": 0
    },
    "gc_raw_num_codons": {
