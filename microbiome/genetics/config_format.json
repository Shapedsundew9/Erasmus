{
    "data_stores": {
        "type": "dict",
        "required": true,
        "valuesrules": {
            "type": "dict",
            "schema": {
                "dbname": {
                    "type": "string",
                    "default": "default"
                },
                "recreate": {
                    "type": "boolean",
                    "default": false
                },
                "temp": {
                    "type": "boolean",
                    "default": false
                },
                "host": {
                    "type": "string",
                    "default": "localhost"
                },
                "username": {
                    "type": "string",
                    "default": "postgres"
                },
                "password": {
                    "type": "string",
                    "default": "postgres"
                },
                "port": {
                    "type": "integer",
                    "default": 5432,
                    "min": 1024, "max": 65535
                }
            }
        }
    },
    "workers": {
        "type": "dict",
        "required": true,
        "keysrules": {
            "regex": "[a-zA-Z-0-9_]{,64}"
        },
        "valuesrules": {
            "type": "dict",
            "schema": {
                "initial_query": {
                    "type": "list",
                    "nullable": true
                },
                "population_limit": {
                    "type": "integer",
                    "min": 1,
                    "max": 2147483647
                }
            }
        }
    }
}