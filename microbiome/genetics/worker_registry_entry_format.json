{
    "name": {
        "type": "string",
        "minlength": 1,
        "maxlength": 64,
        "regex": "[a-zA-Z-0-9_]{,64}",
        "meta": {
            "database": {
                "type": "VARCHAR"
            },
            "definition": {
                "location": ["WORKER"],
                "immutable": true
            },
            "compressed": false,
            "description": "A unique name"
        },
    "initial_query": {
        "type": "string",
        "minlength": 4,
        "check_with": "valid_genomic_library_entry",
        "meta": {
            "database": {
                "type": "VARCHAR"
            },
            "definition": {
                "location": ["WORKER"],
                "immutable": true
            },
            "compressed": true,
            "description": "Query to create the initial population."
        }
    },
    "population_limit": {
        "type": "integer",
        "min": 1,
        "max": 2147483647,
        "meta": {
            "database": {
                "type": "INTEGER"
            },
            "definition": {
                "location": ["WORKER"],
                "immutable": true
            },
            "compressed": false,
            "description": "Maximum population size."
        }
    },
    "population": {
        "type": "string",
        "meta": {
            "database": {
                "type": "BYTEA"
            },
            "definition": {
                "location": ["WORKER"],
                "immutable": true
            },
            "compressed": true,
            "description": "A list of each individuals signature in the population."
        }
    },
    "created": {
        "meta": {
            "database": {
                "type": "TIMESTAMP"
            },
            "definition": {
                "location": ["WORKER"],
                "immutable": true
            },
            "description": "Creation date"
        },
        "type": "string",
        "minlength": 27,
        "maxlength": 27,
        "regex": "20[2-9][0-9]-(1[0-2]|0[1-9])-([0-2][0-9]|3[01])T[0-2][0-9](:[0-5][0-9]){2}\\.[0-9]{6}Z"
    },
    "updated": {
        "meta": {
            "database": {
                "type": "TIMESTAMP"
            },
            "definition": {
                "location": ["WORKER"],
                "immutable": true
            },
            "description": "The time of the last population update for this configuration."
        },
        "type": "string",
        "minlength": 27,
        "maxlength": 27,
        "regex": "20[2-9][0-9]-(1[0-2]|0[1-9])-([0-2][0-9]|3[01])T[0-2][0-9](:[0-5][0-9]){2}\\.[0-9]{6}Z"
    }


}